{
  "name": "biohacking",
  "version": "1.0.0",
  "engines": {
    "node": "7.2.0"
  },
  "description": "Biohacking",
  "main": "server.js",
  "scripts": {
    "postinstall": "node install_production.js",
    "db:create": "node_modules/.bin/create_database",
    "db:drop": "node_modules/.bin/drop_database",
    "db:reset": "npm run db:drop && npm run db:create && sequelize db:migrate && sequelize db:seed:all",
    "start": "nf start -p $PORT",
    "dev": "nf start -p 3000 --procfile Procfile.dev",
    "server": "node --harmony-async-await server",
    "server:dev": "nodemon --harmony-async-await server",
    "server:debug": "nodemon --harmony-async-await --debug-brk --inspect server",
    "eslint": "eslint ./client/src --ext .jsx --ext .js"
  },
  "author": "Insystem",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.15.2",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.3",
    "elasticsearch": "^12.1.0",
    "express": "^4.14.0",
    "express-session": "^1.14.1",
    "foreman": "^2.0.0",
    "le_node": "^1.6.8",
    "moment": "^2.15.2",
    "morgan": "^1.7.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "pg": "^6.1.0",
    "pg-hstore": "^2.3.2",
    "sendgrid": "^4.7.1",
    "sequelize": "^3.24.6",
    "sequelize-cli": "^2.4.0",
    "sequelize-migration-pg-extras": "github:cmilfont/sequelize-migration-pg-extras",
    "serve-favicon": "^2.3.0",
    "uuid": "^2.0.3",
    "winston": "^2.3.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "eslint": "^3.10.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-config-standard-react": "^4.2.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "^6.7.1",
    "eslint-plugin-standard": "^2.0.1",
    "gherkin": "^4.0.0",
    "nodemon": "^1.11.0"
  }
}
